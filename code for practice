/*
----- Function -----
    A. Footer Accordian 
    B. Device Detect
		C. Fleet Slider
		D. Testimonial Slider
*/
	(function (jQuery) {
		jQuery(document).ready(function(){
			deviceDetect();
			footerAcco();
			fleetSlider();
			serviceSlider();
			testimonialSlider();
			keypressNumberOnly();
			viewMoreOnClick();
			faqAccordian();
			/* fleetImageSlider();*/
		/*	readMoreLess(); */
			jQuery('#pickdate').change(function() {
				startDate = jQuery(this).datepicker('getDate');
					jQuery("#returndate").datepicker("option", "minDate", startDate);
			});
			/*
			jQuery('#picktime').change(function() {
				startTime = jQuery(this).timepicker('getTime');
					jQuery("#returntime").timepicker("option", "minTime", startTime);
			});
			*/
			
		});
		
		jQuery(window).on('load', function() {
			equalheight('.fleet-col .elementor-widget-text-editor');
			equalheight('.custom-service-box .services-box-col .elementor-widget-wrap');
			equalheight('.minheightul');
		});
		 jQuery(window).resize(function() {
        setTimeout(function() {
            deviceDetect();		
					equalheight('.fleet-col .elementor-widget-text-editor');
					equalheight('.custom-service-box .services-box-col .elementor-widget-wrap');
					equalheight('.minheightul');
        }, 360);  
			 fleetSlider();
			 serviceSlider();
    });
		/* ------| B. Equal Height |--------- */
var equalheight = function(container) {
	var currentTallest = 0,
		currentRowStart = 0,
		rowDivs = new Array(),
		jQueryel,
		topPosition = 0;
	jQuery(container).each(function() {
		jQueryel = jQuery(this);
		jQuery(jQueryel).height('auto');
		topPostion = jQueryel.position().top;
		if (currentRowStart != topPostion) {
			for (currentDiv = 0; currentDiv < rowDivs.length; currentDiv++) {
				rowDivs[currentDiv].height(currentTallest);
			}
			rowDivs.length = 0; // empty the array
			currentRowStart = topPostion;
			currentTallest = jQueryel.height();
			rowDivs.push(jQueryel);
		} else {
			rowDivs.push(jQueryel);
			currentTallest = (currentTallest < jQueryel.height()) ? (jQueryel.height()) : (currentTallest);
		}
		for (currentDiv = 0; currentDiv < rowDivs.length; currentDiv++) {
			rowDivs[currentDiv].height(currentTallest);
		}
	});
}
		/* ------| A. Footer Accordian |--------- */
    function footerAcco(){
        jQuery('.footer_title').click(function(){
            if(jQuery(this).hasClass('footer_minus')){
                jQuery(this).next().slideUp();jQuery(this).removeClass('footer_minus');
                jQuery(this).parent().removeClass('openBox');
            }else{
                jQuery('.footer_title').removeClass('footer_minus');
                jQuery(this).addClass('footer_minus');
                jQuery('.footer_body').removeClass('openBox');
                jQuery(this).parent().addClass('openBox');
                jQuery('.footer_body').slideUp();
                jQuery(this).next().slideDown();
                setTimeout(function(){
                    jQuery('html,body').animate({
                        scrollTop:jQuery('.openBox').offset().top-70
                    },'slow');
                },1000);
            }
        });
    }
		/* ------| B. Device Detect |--------- */
    function deviceDetect() {
        if(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
            jQuery('body').removeClass('noDeviceDetect');
            jQuery('body').addClass('deviceDetect');
        }else{
            jQuery('body').removeClass('deviceDetect');
            jQuery('body').addClass('noDeviceDetect');
        }
    }
		
		/* ------| C. Fleet Slider |--------- */
		function fleetSlider() {
			if(!jQuery('.fleet-carousel .elementor-container').hasClass('slick-initialized')){
				jQuery('.fleet-carousel .elementor-container').slick({
					dots: true,
					arrows: false,
					infinite: false,
					autoplay:true,
					slidesToShow: 3,
					slidesToScroll: 3,
					adaptiveHeight: true,
					autoplaySpeed: 2000,
					responsive: [
					{
						breakpoint: 9999,
						settings: 'unslick'
					},
					{
						breakpoint: 991,
						settings: {
							slidesToShow: 2,
							slidesToScroll: 2
						}
					},
					{
						breakpoint: 480,
						settings: {
							slidesToShow: 1,
							slidesToScroll: 1
						}
					}
					]
				});
			}
		}
		
		/* ------| C. Fleet Slider |--------- */
		function testimonialSlider() {
			 jQuery('.home-testi-main-slider').slick({
				slidesToShow: 1,
				slidesToScroll: 1,
				arrows: true,
				fade: true,
				asNavFor: '.home-testi-thumb-slider',
				 infinite: true,
				 speed: 300,
				 responsive: [
					 {
						 breakpoint: 768,
						 settings: {
							  adaptiveHeight: true
						 }
					 }
				 ]
			});
			jQuery('.home-testi-thumb-slider').slick({
				slidesToShow: 3,
				slidesToScroll: 1,
				asNavFor: '.home-testi-main-slider',
				dots: false,
				arrows: false,
				centerMode: true,
				focusOnSelect: true,
				centerPadding: '25px',
				infinite: true,
				 speed: 300,
				variableWidth: true,
    variableHeight: true
			});
		}
		
		/*-----------| F. Keypress Number Only  |-----------*/
    function keypressNumberOnly() {
        jQuery('.numberOnly').keypress(function(evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57) && charCode !== 8) {
                return false;
            }
            if (charCode === 46) {
                return false;
            }
            return true;
        });
    }
		
		
				function serviceSlider() {
			if(!jQuery('.custom-service-box .e-con-inner').hasClass('slick-initialized')){
				jQuery('.custom-service-box .e-con-inner').slick({
					dots: true,
					arrows: false,
					infinite: false,
					autoplay:false,
					slidesToShow: 3,
					slidesToScroll: 3,
					adaptiveHeight: true,
					autoplaySpeed: 2000,
					responsive: [
					{
						breakpoint: 9999,
						settings: 'unslick'
					},
					{
						breakpoint: 991,
						settings: {
							slidesToShow: 2,
							slidesToScroll: 2
						}
					},
					{
						breakpoint: 480,
						settings: {
							slidesToShow: 1,
							slidesToScroll: 1
						}
					}
					]
				});
			}
		}
		
		
		function viewMoreOnClick(){
			jQuery('#view-more-btn').click(function(){	
 				jQuery('.custom-service-box .view-more-box').last().slideToggle();
				if (jQuery('#view-more-btn .elementor-button-content-wrapper .elementor-button-text').text() === 'View More') {
						jQuery('#view-more-btn .elementor-button-content-wrapper .elementor-button-text').text('View Less');
					} else {
						jQuery('#view-more-btn .elementor-button-content-wrapper .elementor-button-text').text('View More');
					}
			});
		}
		
		
		function faqAccordian() {
			jQuery('.accordian-page .acco-title-1').addClass('acco-open');
	    jQuery('.accordian-page .acco-body-1').show();
	jQuery('.accordian-page .acco-title').click(function() {
		if (jQuery(this).hasClass('acco-open')) {
			jQuery(this).next().slideUp();
			jQuery(this).removeClass('acco-open');
		} else {
			jQuery('.accordian-page .acco-title').removeClass('acco-open');
			jQuery(this).addClass('acco-open');
			jQuery('.accordian-page .acco-body').slideUp();
			jQuery(this).next().slideDown();
		}
	});
}


		
	}(jQuery));



