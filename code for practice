	jQuery(document).ready(function () {
		jQuery(".menu-btn a").click(function (event) {
			event.preventDefault();
			jQuery(".nav_menu").slideToggle(1000); 
			jQuery(this).toggleClass('btn-open').toggleClass('btn-close');
		});
	
		jQuery('.nav_menu').on('click', function (event) {
			event.stopPropagation(); 
		});
	
		jQuery('.menu a').on('click', function () {
			jQuery(".nav_menu").slideUp(1000); 
			jQuery(".menu-btn a").removeClass('btn-close').addClass('btn-open'); 
		});
	
		
		jQuery(document).click(function (event) {
			if (!jQuery(event.target).closest('.nav_menu, .menu-btn').length) {
				jQuery(".nav_menu").slideUp(1000); 
				jQuery(".menu-btn a").removeClass('btn-close').addClass('btn-open'); 
			}
		});
	});



	
